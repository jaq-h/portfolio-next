{
  "name": "portfolio-next",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev -p 3001 --turbopack",
    "dev:clean": "npm run clean && next dev -p 3001",
    "build": "next build",
    "build:clean": "npm run clean && next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf .next node_modules/.cache",
    "clean:all": "rm -rf .next node_modules/.cache .turbo",
    "fresh": "rm -rf node_modules package-lock.json .next && npm install",
    "prebuild": "npm run clean",
    "migrate:edge-config": "tsx scripts/migrate-to-edge-config.ts",
    "sync:resume": "tsx scripts/sync-resume.ts",
    "sync:media": "tsx scripts/sync-media.ts",
    "sync:images": "tsx scripts/sync-media.ts --images",
    "sync:icons": "tsx scripts/sync-media.ts --icons",
    "revalidate": "curl -X POST http://localhost:3000/api/revalidate -H 'Authorization: Bearer $REVALIDATION_SECRET' -H 'Content-Type: application/json' -d '{\"all\": true}'"
  },
  "dependencies": {
    "@vercel/blob": "^2.0.1",
    "@vercel/edge-config": "^1.4.3",
    "critters": "^0.0.23",
    "next": "^15.1.4",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "sharp": "^0.34.5",
    "tsx": "^4.21.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@types/node": "^22.15.30",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.6",
    "dotenv": "^17.2.3",
    "eslint": "^9.28.0",
    "eslint-config-next": "^15.1.4",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.19",
    "typescript": "^5.9.3"
  }
}
